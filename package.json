{
  "name": "sote2035",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -k -n server,client -c auto \"npm:dev -w server\" \"npm:dev -w client\"",
    "build": "npm -w server run build && npm -w client run build",
    "start": "concurrently -k -n server,client -c auto \"npm:start -w server\" \"npm:start -w client\"",
    "lint": "npm run -w client lint",
    "typecheck": "npm run -w client typecheck"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  }
}
